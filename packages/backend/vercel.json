{
  "version": 2,
  "builds": [
    { 
      "src": "src/main.ts", 
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/swagger-ui-dist/**"]
      }
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "src/main.ts", 
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"] 
    }
  ]
}
